{
    "name": "sofyco/json-request-bundle",
    "type": "symfony-bundle",
    "description": "Symfony JSON Request Bundle",
    "keywords": ["php", "symfony", "http", "json", "request", "bundle"],
    "homepage": "https://github.com/sofyco/json-request-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofy Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "symfony/framework-bundle": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/serializer": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\JsonRequestBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\JsonRequestBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
